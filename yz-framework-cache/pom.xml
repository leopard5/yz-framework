            <groupId>com.yz</groupId>
            <artifactId>yz-framework-core</artifactId>
            <version>${yz-framework-core.version}</version>
        </dependency>
        <dependency>
            <groupId>com.yz</groupId>
            <artifactId>yz-framework-zookeeper</artifactId>
            <version>${yz-framework-zookeeper.version}</version>
